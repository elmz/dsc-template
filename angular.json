{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dsc-template": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/dsc-template",
      "sourceRoot": "projects/dsc-template/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/dsc-template",
            "index": "projects/dsc-template/src/index.html",
            "main": "projects/dsc-template/src/main.ts",
            "polyfills": "projects/dsc-template/src/polyfills.ts",
            "tsConfig": "projects/dsc-template/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/dsc-template/src/favicon.ico",
              "projects/dsc-template/src/assets"
            ],
            "styles": [
              "./node_modules/dsc-alert/dsc-alert.component.css",
              "projects/dsc-template/src/styles.css"
            ],
            "scripts": ["./node_modules/@clr/icons/clr-icons.min.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/dsc-template/src/environments/environment.ts",
                  "with": "projects/dsc-template/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dsc-template:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dsc-template:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dsc-template:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dsc-template/src/test.ts",
            "polyfills": "projects/dsc-template/src/polyfills.ts",
            "tsConfig": "projects/dsc-template/tsconfig.spec.json",
            "karmaConfig": "projects/dsc-template/karma.conf.js",
            "assets": [
              "projects/dsc-template/src/favicon.ico",
              "projects/dsc-template/src/assets"
            ],
            "styles": [
              "projects/dsc-template/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dsc-template/tsconfig.app.json",
              "projects/dsc-template/tsconfig.spec.json",
              "projects/dsc-template/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/dsc-template/e2e/protractor.conf.js",
            "devServerTarget": "dsc-template:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dsc-template:serve:production"
            }
          }
        }
      }
    },
    "dsc-alert": {
      "projectType": "library",
      "root": "projects/dsc-alert",
      "sourceRoot": "projects/dsc-alert/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
